@article{carey_phage_2019,
	title = {Phage integration alters the respiratory strategy of its host},
	volume = {8},
	issn = {2050-084X},
	url = {https://doi.org/10.7554/eLife.49081},
	doi = {10.7554/eLife.49081},
	abstract = {Temperate bacteriophages are viruses that can incorporate their genomes into their bacterial hosts, existing there as prophages that refrain from killing the host cell until induced. Prophages are largely quiescent, but they can alter host phenotype through factors encoded in their genomes (often virulence factors) or by disrupting host genes as a result of integration. Here we describe another mechanism by which a prophage can modulate host phenotype. We show that a temperate phage that integrates in Escherichia coli reprograms host regulation of an anaerobic respiratory system, thereby inhibiting a bet hedging strategy. The phage exerts this effect by upregulating a host-encoded signal transduction protein through transcription initiated from a phage-encoded promoter. We further show that this phenomenon occurs not only in a laboratory strain of E. coli, but also in a natural isolate that contains a prophage at this site.},
	urldate = {2020-04-22},
	journal = {eLife},
	author = {Carey, Jeffrey N and Mettert, Erin L and Fishman-Engel, Daniel R and Roggiani, Manuela and Kiley, Patricia J and Goulian, Mark},
	editor = {Laub, Michael T and Storz, Gisela},
	month = oct,
	year = {2019},
	keywords = {bacteriophage, bet hedging, trimethylamine N-oxide, two-component signaling},
	pages = {e49081},
	file = {Full Text PDF:/Users/adamhockenberry/Zotero/storage/ZY42SIHR/Carey et al. - 2019 - Phage integration alters the respiratory strategy .pdf:application/pdf}
}

@article{brum_patterns_2015,
	title = {Patterns and ecological drivers of ocean viral communities},
	volume = {348},
	copyright = {Copyright © 2015, American Association for the Advancement of Science},
	issn = {0036-8075, 1095-9203},
	url = {https://science.sciencemag.org/content/348/6237/1261498},
