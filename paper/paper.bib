	note = {Publisher: Cold Spring Harbor Laboratory
Section: New Results},
	pages = {2020.07.11.198606},
	file = {Full Text PDF:/Users/adamhockenberry/Zotero/storage/P7K7HJFC/Tynecki et al. - 2020 - PhageAI - Bacteriophage Life Cycle Recognition wit.pdf:application/pdf;Snapshot:/Users/adamhockenberry/Zotero/storage/3A4IF2V3/2020.07.11.html:text/html}
